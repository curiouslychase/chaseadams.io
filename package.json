{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local:link-content": "test -L src/content || ln -s $HOME/Dropbox/chaseadams.io/content src/content",
    "dev": "next dev",
    "lint": "eslint . --ext '**/*.{js,jsx,ts,tsx,json}' --max-warnings=0",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "start": "next start",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "build": "yarn build:get-content && yarn build:rss && yarn build:next",
    "build:get-content": "node scripts/get-content/index.js --fromDropbox=/chaseadams.io/content --dest=src",
    "build:next": "next build",
    "build:rss": "tsc --outDir scripts/generate-rss/dist --p scripts/generate-rss && node scripts/generate-rss/dist/scripts/generate-rss/index.js"
  },
  "dependencies": {
    "@szhsin/react-menu": "^1.6.0",
    "babel-plugin-styled-components": "npm:@z1digitalstudio/babel-plugin-styled-components@1.11.1-rev.1",
    "date-fns": "^2.16.1",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "mdx-prism": "^0.3.1",
    "next": "^10.0.0",
    "next-mdx-enhanced": "^5.0.0",
    "next-mdx-remote": "^2.1.3",
    "prismjs": "^1.22.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rehype": "^11.0.0",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-code-titles": "^0.1.1",
    "remark-html": "^13.0.1",
    "remark-prism": "^1.3.4",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.2.0",
    "rss": "^1.2.2",
    "slugify": "^1.4.6",
    "styled-components": "^5.2.1",
    "stylelint-order": "^4.1.0",
    "ts-node": "^9.1.0",
    "tsc": "^1.20150623.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/github-slugger": "^1.3.0",
    "@types/google.analytics": "^0.0.41",
    "@types/node": "^14.14.10",
    "@types/prismjs": "^1.16.3",
    "@types/react": "^16.9.56",
    "@types/rss": "^0.0.28",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "dropbox": "^4.0.30",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-zip": "^1.6.7",
    "husky": "^4.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.0.5",
    "yargs": "^15.1.0"
  }
}
